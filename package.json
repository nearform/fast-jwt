{
  "name": "fast-jwt",
  "version": "4.0.6",
  "description": "Fast JSON Web Token implementation",
  "author": "NearForm Ltd",
  "homepage": "https://github.com/nearform/fast-jwt",
  "contributors": [
    {
      "name": "Paolo Insogna",
      "url": "https://github.com/ShogunPanda"
    },
    {
      "name": "Matteo Collina",
      "email": "hello@matteocollina.com"
